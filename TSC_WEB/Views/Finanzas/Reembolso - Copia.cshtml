            function conceptoCabXTipo(pIdTipo, pIdConceptoDet) {

                let firstConceptoCab = 0;
                let firstLoop = true;
                let conceptoArray = [];

                let v_codConceptoCab = 0;
                let v_cantidadDetalle = 0;

                let newCabeceraArray = [];
                let newDetalleArray = [];


                if (pIdConceptoDet > 0) {

                    $("#cbxConcepCab").empty();

                    v_codConceptoCab = conceptoDetalleArray.filter(x => x.idConceptoDet == pIdConceptoDet).idConceptoCab;

                    v_cantidadDetalle = conceptoDetalleArray.filter(x => x.idConcepDet != pIdConceptoDet).length;

                    if (v_cantidadDetalle > 0) {
                        $("#cbxConcepCab").val(v_codConceptoCab);
                        newDetalleArray = conceptoDetalleArray.filter(x => x.idConcepDet != pIdConceptoDet);
                    }
                    else {
                        newCabeceraArray = conceptoCabeceraArray.filter(x => x.idConcepCab != v_codConceptoCab);
                        newDetalleArray = conceptoDetalleArray.filter(x => x.idConcepDet != pIdConceptoDet);
                    }


                    llenarCbxDetalle(newDetalleArray);

                    $.each(conceptoArray, function () {
                        if (firstLoop) {
                            firstConceptoCab = this.idConceptoCab;

                            firstLoop = false;
                        }
                        $("#cbxConcepCab").append($("<option/>").val(this.idConceptoCab).text(this.conceptoCab));
                    });

                    vIdConcepCab = firstConceptoCab;

                    $("#cbxConcepCab").val(firstConceptoCab);
                    getConceptoDet(firstConceptoCab);
                }

            }

