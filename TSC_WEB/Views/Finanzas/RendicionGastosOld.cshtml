@section scripts {
    <script>

        const USUARIO = "@Request.RequestContext.HttpContext.Session["usuario"]";
        const USUARIO_COMPLETO = "@Request.RequestContext.HttpContext.Session["usuario"]" + " - " + "@Request.RequestContext.HttpContext.Session["nombre"]";
        const PORCENTAJE_IGV = 18;

        var vTipoComprobante1 = [];

        var solicitudSeleccionada = {};
        var vIdSolicitud = 0;
        var vCeCo = 0;
        var vIdRendCompte = 0;

        var iDetalle1 = 0;
        var iDetalle2 = 0;
        var iDetalle3 = 0;

        var proveedor = {};
        var detalleArray = [];
        var detalleArray3 = [];
        var detPlaMov = {};

        var detComprobanteArray1 = [];
        var detComprobanteArray2 = [];
        var detComprobanteArray3 = [];

        var edetComprobanteArray1 = [];
        var edetComprobanteArray2 = [];
        var edetComprobanteArray3 = [];

        var tabSeleccionado = 1; // 1: Factura y Boleta, 2: Planilla Movilidad, 3: Planilla de Movilidada

        var listaComptesIdTipoComp = 0;

        var idsolicitud_dev = 0;
        var idsecuencia_dev = 0;

        var TIPO_REGISTRO = "";
        var tiposComprobantesArray = [];

        $(document).ready(function () {

            Inicializar();

            function Inicializar() {

                getTipoComprobantes();
                getTipoMoneda();
                getUnidadesMedida();
                getPeriodos();

                $("#regComprobModal .kpi-modal-monto1").css("background-color", "#ABB2B9");
                $("#regComprobModal .kpi-modal-monto2").css("background-color", "#ABB2B9");
                $("#regComprobModal .kpi-modal-monto3").css("background-color", "#ABB2B9");

                $("#txtCodigoModal3").val("0");
                $("#txtNumPlanillaModal2").val("0");

            }


            function setPeriodo() {

                let fechaFormato = new Date();

                let month = '' + (fechaFormato.getMonth() + 1);
                let year = fechaFormato.getFullYear();

                if (month.length < 2) { month = '0' + month; }

                $("#cbxPeriodoModal1").val("" + [year, month].join('-'));
                $("#cbxPeriodoModal2").val("" + [year, month].join('-'));
                $("#cbxPeriodoModal3").val("" + [year, month].join('-'));

                document.getElementById("cbxPeriodoModal1").disabled = true;
                document.getElementById("cbxPeriodoModal2").disabled = true;
                document.getElementById("cbxPeriodoModal3").disabled = true;
            }


            function getTipoMoneda() {
                $.ajax({
                    type: "GET",
                    url: '/Finanzas/RG_TiposDeMoneda',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $("#cbxMonedaModal1").empty();
                        $("#cbxMonedaModal2").empty();
                        $("#cbxMonedaModal3").empty();

                        $.each(data.lista, function () {
                            $("#cbxMonedaModal1").append($("<option/>").val(this.idTipoMod).text(this.descripcion));
                            $("#cbxMonedaModal2").append($("<option/>").val(this.idTipoMod).text(this.descripcion));
                            $("#cbxMonedaModal3").append($("<option/>").val(this.idTipoMod).text(this.descripcion));
                        });

                        $("#cbxMonedaModal1").val(1);
                        $("#cbxMonedaModal2").val(1);
                        $("#cbxMonedaModal3").val(1);

                    },
                    failure: function () {
                        console.error('error al cargar tipos periodos');
                    }
                });
            }

            function getPeriodos() {

                $.ajax({
                    type: "GET",
                    url: '/Finanzas/RG_Periodos',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {

                        $("#cbxPeriodoModal1").empty();
                        $("#cbxPeriodoModal2").empty();
                        $("#cbxPeriodoModal3").empty();

                        $.each(data.lista, function () {
                            $("#cbxPeriodoModal1").append($("<option/>").val(this.periodo).text(this.periodo));
                            $("#cbxPeriodoModal2").append($("<option/>").val(this.periodo).text(this.periodo));
                            $("#cbxPeriodoModal3").append($("<option/>").val(this.periodo).text(this.periodo));
                        });

                        setPeriodo();
                    },
                    failure: function () {
                        console.error('error al cargar tipos periodos');
                    }
                });
            }

            function getUnidadesMedida() {

                $.ajax({
                    type: "GET",
                    url: '/Finanzas/RG_UnidadesMedida',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $("#cbxUM1").empty();

                        $.each(data.lista, function () {
                            $("#cbxUM1").append($("<option/>").val(this.codum).text(this.um));
                        });

                        $("#cbxUM1").val("UN");
                    },
                    failure: function () {
                        console.error('error al cargar tipos periodos');
                    }
                });

            }

            function getTipoComprobantes() {
                $.ajax({
                    type: "GET",
                    url: '/Finanzas/RG_TipoComprobantes',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        let primerValorBool = true;

                        tiposComprobantesArray = data.lista;

                        $("#cbxTipoCpbteCompModal1").empty();
                        $("#cbxTipoCpbteCompModal2").empty();
                        $("#cbxTipoCpbteCompModal3").empty();


                        $.each(data.lista, function () {
                            if (primerValorBool) {
                                vCodTipoComprobante = this.idTipoComp;
                            }

                            if (this.idTipoComp == 1 || this.idTipoComp == 2) {
                                $("#cbxTipoCpbteCompModal1").append($("<option/>").val(this.idTipoComp).text(this.tipoComprobante));
                            }
                            else if (this.idTipoComp == 3) {
                                $("#cbxTipoCpbteCompModal2").append($("<option/>").val(this.idTipoComp).text(this.tipoComprobante));
                            }
                            else if (this.idTipoComp == 4) {
                                $("#cbxTipoCpbteCompModal3").append($("<option/>").val(this.idTipoComp).text(this.tipoComprobante));
                            }

                            primerValorBool = false;
                        });

                        cambioTipoCpte(vCodTipoComprobante);

                        $("#cbxTipoCpbteCompModal1").val(vCodTipoComprobante); // Factura o boleta.
                        $("#cbxTipoCpbteCompModal2").val(3); // Planilla de Movilidad
                        $("#cbxTipoCpbteCompModal3").val(4); // Planilla de Movilidad

