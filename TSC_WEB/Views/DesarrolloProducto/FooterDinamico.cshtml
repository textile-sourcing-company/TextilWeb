
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FooterDinamico</title>
</head>
<body style="width:100%;padding:0;margin:0;">
    <div style="border-top:1px solid black; width:50%; text-align:left;float:left"> 

        <h5>PC: @ViewBag.usuario</h5>  

    </div>
    <div style="border-top:1px solid black; width:50%; text-align:right;float:left">

        <h5>@DateTime.Now.ToString("MM/dd/yyyy hh:mm tt")</h5>

    </div>
</body>
</html>
