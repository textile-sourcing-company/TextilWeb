                       {
                           obj.DESCR_COMBINACAO,
                       }).Distinct().ToList();

    Decimal cantidadCorte = CORTE1.Count;
    Decimal modCorte = cantidadCorte / 8;
    int contadorCorte = 1, contadorCorte2 = 0, contadorCorte3 = 0;




    //COSTURA
