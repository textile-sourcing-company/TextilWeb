                            <th>UBICACIÓN</th>
                            <th>% DEF</th>
                            <th>TALLA</th>
                            <th>CONSUMO</th>
                            @{
                                contador = x * 8;
                                contador2 = 0;
                            }
                            @foreach (var obj in COMBINACIONESLISTADISTINCT)
                            {
                                contador2++;
                                if ((contador2 <= contador) && (contador2 > (contador - 8)))
                                {
                                    <th>@obj.DESCR_COMBINACAO @obj.COMBINACAO_ITEM</th>
                                }
                            }
                        </tr>
                    </thead>
                    <tbody>
                        @{ contador2 = 0;}
