                               obj.NARRATIVA,
                               obj.TALLA_A,
                               obj.CODIGO2,
                               obj.DESCRICAO_01,
                               obj.OBSERVACION,
                               obj.UNIDADE_MEDIDA,
                               obj.CONSUMO,
                               obj.DESCR_COMBINACAO,
                               obj.DESCRICAO_15
                           }

                  ).Distinct().ToList();
    var COSTURACOLOR = (from obj in Model.objAviosLista
                        where obj.ESTAGIO == "2"
                        select new
                        {
                            obj.DESCR_COMBINACAO,
                        }).Distinct().ToList();

    //ACABADOS
    var ACABADOS = (from obj in Model.objAviosLista
                    where obj.ESTAGIO == "6"
                    select new
                    {
                        obj.NARRATIVA,
                        obj.TALLA_A,
                        obj.CODIGO2,
                        obj.DESCRICAO_01,
                        obj.OBSERVACION,
                        obj.UNIDADE_MEDIDA,
                        obj.CONSUMO,
                    }).Distinct().ToList();

    var ACABADOSDISTINCT = (from obj in Model.objAviosLista
                            where obj.ESTAGIO == "6"
                            select new
